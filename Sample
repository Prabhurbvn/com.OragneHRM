public class Demo
{
  
   public void add(int a, int b)
   {
   
    Sysout(a+b);
    
    }
    
    public void add(float a, float b, float c)
    {
    Sysout(a+b+c);
    
    }
    
    
    public void div(int a, int b)
    {
    sysout(a/b);
    }
    public void(int a, int b)
    {
    Sysout(a-b);
    }
     

}
